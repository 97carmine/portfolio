{
	"name": "portfolio",
	"version": "1.0.0",
	"description": "Axel Gabriel Calle Granda's portfolio",
	"private": true,
	"author": "Axel Gabriel Calle Granda <97carmine@protonmail.com> (https://github.com/97carmine)",
	"license": "MIT",
	"repository": "github:97carmine/portfolio",
	"bugs": {
		"url": "https://github.com/97carmine/portfolio/issues"
	},
	"homepage": "https://github.com/97carmine/portfolio",
	"browserslist": [
		"defaults"
	],
	"packageManager": "yarn@3.2.0",
	"scripts": {
		"extract:es": "formatjs extract './src/**/*.{ts,tsx}' --ignore ./src/@types --id-interpolation-pattern '[contenthash:5]' --out-file ./src/common/languages/templates/es.json",
		"extract:en": "formatjs extract './src/**/*.{ts,tsx}' --ignore ./src/@types --id-interpolation-pattern '[contenthash:5]' --out-file ./src/common/languages/templates/en.json",
		"compile:es": "formatjs compile ./src/common/languages/templates/es.json --ast --out-file ./src/common/languages/messages/es.json",
		"compile:en": "formatjs compile ./src/common/languages/templates/en.json --ast --out-file ./src/common/languages/messages/en.json",
		"build:prod": "webpack --config webpack.prod.ts",
		"build:dev": "webpack --config webpack.dev.ts",
		"start:prod": "NODE_ENV=production node build/server.js",
		"start:dev": "nodemon build/server.js"
	},
	"dependencies": {
		"ajv": "8.8.2",
		"body-parser": "1.19.0",
		"bulma": "0.9.3",
		"dotenv": "16.0.0",
		"express": "4.17.1",
		"helmet": "5.0.1",
		"nodemailer": "6.7.2",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-intl": "5.24.1",
		"react-router-dom": "6.2.1",
		"react-transition-group": "4.4.2"
	},
	"devDependencies": {
		"@axe-core/react": "4.4.0",
		"@formatjs/cli": "4.7.0",
		"@formatjs/ts-transformer": "3.8.1",
		"@types/body-parser": "1.19.2",
		"@types/copy-webpack-plugin": "8.0.1",
		"@types/express": "4.17.13",
		"@types/html-minifier-terser": "6.1.0",
		"@types/node": "16.11.9",
		"@types/nodemailer": "6.4.4",
		"@types/react": "17.0.36",
		"@types/react-dom": "17.0.11",
		"@types/react-router-dom": "5.3.3",
		"@types/react-transition-group": "4.4.4",
		"@types/webpack-assets-manifest": "5.0.2",
		"@typescript-eslint/eslint-plugin": "5.4.0",
		"@typescript-eslint/parser": "5.4.0",
		"copy-webpack-plugin": "9.1.0",
		"css-loader": "6.3.0",
		"css-minimizer-webpack-plugin": "3.2.0",
		"eslint": "8.2.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-formatjs": "2.20.1",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.27.0",
		"eslint-webpack-plugin": "3.1.1",
		"eta": "1.12.3",
		"favicons-webpack-plugin": "5.0.2",
		"html-minifier-terser": "6.1.0",
		"mini-css-extract-plugin": "2.5.2",
		"nodemon": "2.0.15",
		"prettier": "2.3.2",
		"sass": "1.42.1",
		"sass-loader": "12.0.0",
		"stylelint": "14.1.0",
		"stylelint-config-prettier": "9.0.3",
		"stylelint-prettier": "2.0.0",
		"stylelint-webpack-plugin": "3.1.0",
		"ts-loader": "9.2.6",
		"ts-node": "10.4.0",
		"typescript": "4.5.2",
		"webpack": "5.64.4",
		"webpack-assets-manifest": "5.0.6",
		"webpack-cli": "4.9.2"
	}
}
