{
	"name": "portfolio",
	"version": "1.0.0",
	"description": "Axel Gabriel Calle Granda's portfolio",
	"private": true,
	"author": "Axel Gabriel Calle Granda <97carmine@protonmail.com> (https://github.com/97carmine)",
	"license": "MIT",
	"repository": "github:97carmine/portfolio",
	"bugs": {
		"url": "https://github.com/97carmine/portfolio/issues"
	},
	"homepage": "https://github.com/97carmine/portfolio",
	"browserslist": [
		"defaults"
	],
	"packageManager": "yarn@3.2.2",
	"scripts": {
		"extract:es": "formatjs extract './src/**/*.{ts,tsx}' --ignore ./src/@types --id-interpolation-pattern '[contenthash:5]' --out-file ./src/common/languages/templates/es.json",
		"extract:en": "formatjs extract './src/**/*.{ts,tsx}' --ignore ./src/@types --id-interpolation-pattern '[contenthash:5]' --out-file ./src/common/languages/templates/en.json",
		"compile:es": "formatjs compile ./src/common/languages/templates/es.json --ast --out-file ./src/common/languages/messages/es.json",
		"compile:en": "formatjs compile ./src/common/languages/templates/en.json --ast --out-file ./src/common/languages/messages/en.json",
		"build:prod": "webpack --config webpack.prod.ts",
		"build:dev": "webpack --config webpack.dev.ts",
		"start:prod": "NODE_ENV=production node build/server.js",
		"start:dev": "nodemon build/server.js"
	},
	"dependencies": {
		"ajv": "8.11.0",
		"body-parser": "1.20.0",
		"bulma": "0.9.4",
		"dotenv": "16.0.0",
		"express": "4.17.3",
		"helmet": "5.0.2",
		"nodemailer": "6.7.3",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-intl": "5.25.0",
		"react-router-dom": "6.3.0",
		"react-transition-group": "4.4.2"
	},
	"devDependencies": {
		"@axe-core/react": "4.4.3",
		"@formatjs/cli": "4.8.3",
		"@formatjs/ts-transformer": "3.9.3",
		"@types/body-parser": "1.19.2",
		"@types/express": "4.17.13",
		"@types/html-minifier-terser": "6.1.0",
		"@types/node": "16.11.27",
		"@types/nodemailer": "6.4.4",
		"@types/react": "17.0.44",
		"@types/react-dom": "17.0.16",
		"@types/react-router-dom": "5.3.3",
		"@types/react-transition-group": "4.4.4",
		"@types/webpack-assets-manifest": "5.1.0",
		"@typescript-eslint/eslint-plugin": "5.20.0",
		"@typescript-eslint/parser": "5.20.0",
		"copy-webpack-plugin": "10.2.4",
		"css-loader": "6.7.1",
		"css-minimizer-webpack-plugin": "3.4.1",
		"eslint": "8.13.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-formatjs": "3.1.0",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.29.4",
		"eslint-webpack-plugin": "3.1.1",
		"eta": "1.12.3",
		"favicons-webpack-plugin": "5.0.2",
		"html-minifier-terser": "6.1.0",
		"mini-css-extract-plugin": "2.6.0",
		"nodemon": "2.0.15",
		"prettier": "2.6.2",
		"sass": "1.50.1",
		"sass-loader": "12.6.0",
		"stylelint": "14.7.1",
		"stylelint-config-prettier": "9.0.3",
		"stylelint-prettier": "2.0.0",
		"stylelint-webpack-plugin": "3.2.0",
		"ts-loader": "9.2.8",
		"ts-node": "10.7.0",
		"typescript": "4.6.3",
		"webpack": "5.72.0",
		"webpack-assets-manifest": "5.1.0",
		"webpack-cli": "4.9.2"
	}
}
