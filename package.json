{
	"name": "portfolio",
	"version": "1.0.0",
	"description": "Axel Gabriel Calle Granda's portfolio",
	"private": true,
	"author": "Axel Gabriel Calle Granda <97carmine@protonmail.com> (https://github.com/97carmine)",
	"license": "MIT",
	"repository": "github:97carmine/portfolio",
	"bugs": {
		"url": "https://github.com/97carmine/portfolio/issues"
	},
	"homepage": "https://github.com/97carmine/portfolio",
	"browserslist": [
		"defaults"
	],
	"packageManager": "yarn@2.4.2",
	"scripts": {
		"extract:es": "formatjs extract './src/**/*.{ts,tsx}' --ignore ./src/@types --id-interpolation-pattern '[contenthash:5]' --out-file ./src/common/languages/templates/es.json",
		"extract:en": "formatjs extract './src/**/*.{ts,tsx}' --ignore ./src/@types --id-interpolation-pattern '[contenthash:5]' --out-file ./src/common/languages/templates/en.json",
		"compile:es": "formatjs compile ./src/common/languages/templates/es.json --ast --out-file ./src/common/languages/messages/es.json",
		"compile:en": "formatjs compile ./src/common/languages/templates/en.json --ast --out-file ./src/common/languages/messages/en.json",
		"build:prod": "webpack --config webpack.prod.ts",
		"build:dev": "webpack --config webpack.dev.ts",
		"start:prod": "node build/server.js",
		"start:dev": "nodemon build/server.js"
	},
	"dependencies": {
		"ajv": "8.6.0",
		"body-parser": "1.19.0",
		"bulma": "0.9.2",
		"dotenv": "10.0.0",
		"express": "4.17.1",
		"helmet": "4.6.0",
		"nodemailer": "6.6.2",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-intl": "5.19.0",
		"react-router": "5.2.0",
		"react-router-dom": "5.2.0",
		"react-transition-group": "4.4.2"
	},
	"devDependencies": {
		"@axe-core/react": "4.2.2",
		"@formatjs/cli": "4.2.18",
		"@formatjs/ts-transformer": "3.4.2",
		"@types/body-parser": "1.19.0",
		"@types/copy-webpack-plugin": "8.0.1",
		"@types/css-minimizer-webpack-plugin": "3.0.1",
		"@types/express": "4.17.13",
		"@types/html-minifier-terser": "5.1.2",
		"@types/mini-css-extract-plugin": "2.0.0",
		"@types/node": "15.12.2",
		"@types/nodemailer": "6.4.2",
		"@types/react": "17.0.14",
		"@types/react-dom": "17.0.9",
		"@types/react-router": "5.1.16",
		"@types/react-router-dom": "5.1.8",
		"@types/react-transition-group": "4.4.2",
		"@types/webpack-assets-manifest": "5.0.1",
		"@typescript-eslint/eslint-plugin": "4.26.1",
		"@typescript-eslint/parser": "4.26.1",
		"copy-webpack-plugin": "9.0.1",
		"css-loader": "5.2.6",
		"css-minimizer-webpack-plugin": "3.0.2",
		"eslint": "7.27.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-formatjs": "2.15.5",
		"eslint-plugin-prettier": "3.4.0",
		"eslint-plugin-react": "7.24.0",
		"eslint-webpack-plugin": "2.5.4",
		"eta": "1.12.3",
		"favicons": "6.2.2",
		"favicons-webpack-plugin": "5.0.2",
		"html-minifier-terser": "5.1.1",
		"mini-css-extract-plugin": "2.0.0",
		"nodemon": "2.0.9",
		"prettier": "2.3.2",
		"sass": "1.32.13",
		"sass-loader": "12.0.0",
		"stylelint": "13.13.1",
		"stylelint-config-prettier": "8.0.2",
		"stylelint-prettier": "1.2.0",
		"stylelint-webpack-plugin": "2.2.2",
		"ts-loader": "9.2.3",
		"ts-node": "10.0.0",
		"typescript": "4.3.2",
		"webpack": "5.41.1",
		"webpack-assets-manifest": "5.0.6",
		"webpack-cli": "4.7.2"
	}
}
