<!DOCTYPE html>
<html lang="<%= it.language %>">
	<head>
		<meta charset="utf-8">
		<meta name="keywords" content="<%= it.keywords %>">
		<meta name="author" content="Axel Gabriel Calle Granda">
		<meta name="description" content="<%= it.description %>">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<title>Axel Gabriel Calle Granda</title>
		<link rel="canonical" href="<%= it.canonical_URL %>">
		<link type="text/plain" rel="author" href="<%= it.humans_URL %>">
		<!-- Favicons -->
		<% it.assets
			.filter(([key]) => key === "favicon.png")
			.map((data) => {%>
				<link type="image/png" rel="icon" href="<%= process.env.NODE_ENV === 'production' ? data[1].src : data[1] %>">
			<% }) %>
		<!-- CSS -->
		<% it.assets
			.filter(([key]) => /^([a-zA-Z0-9./])+(css)$/.test(key))
			.map((data) => {
				if (process.env.NODE_ENV === "production") { %>
					<link type="text/css" rel="stylesheet" href="<%= data[1].src %>" integrity="<%= data[1].integrity %>" crossorigin="anonymous">
				<% } else { %>
					<link type="text/css" rel="stylesheet" href="<%= data[1] %>">
			<% }}) %>
		<!-- JS -->
		<% it.assets
			.filter(([key]) => /^([a-zA-Z0-9./])+(js)$/.test(key))
			.map((data) => {
				if (process.env.NODE_ENV === "production") { %>
					<script type="text/javascript" async src="<%= data[1].src %>" integrity="<%= data[1].integrity %>" crossorigin="anonymous"></script>
				<% } else { %>
					<script type="text/javascript" async src="<%= data[1] %>"></script>
			<% }}) %>
	</head>
	<body>
		<noscript><div class="noJS"><%= it.noJS %></div></noscript>
		<div id="root"><%~ it.component %></div>
	</body>
</html>